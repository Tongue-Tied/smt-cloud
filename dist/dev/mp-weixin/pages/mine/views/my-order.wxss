page {
  background: #f5f5f5;
  padding-bottom: calc(20rpx + env(safe-area-inset-bottom));
}
.my_order .my_tabs {
  background: #ffffff;
}
.my_order .content {
  padding: 0 40rpx;
}
.my_order .content .co_item {
  width: 100%;
  margin: 0 auto;
  background: #ffffff;
  margin-top: 30rpx;
  border-radius: 40rpx;
}
.my_order .content .co_item .bottom {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding-top: 20rpx;
  padding: 30rpx;
}
.my_order .content .co_item .bottom .right {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  margin-left: 20rpx;
}
.my_order .content .co_item .bottom .right .right_top {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.my_order .content .co_item .bottom .right .right_top .rt_left {
  color: #999999;
  font-size: 24rpx;
}
.my_order .content .co_item .bottom .right .right_top .rt_left .title {
  width: 90%;
  color: #333333;
  overflow: hidden;
}
.my_order .content .co_item .bottom .right .right_top .rt_right {
  text-align: right;
  height: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.my_order .content .co_item .bottom .right .right_top .rt_right :nth-child(1) {
  font-size: 26rpx;
}
.my_order .content .co_item .bottom .right .right_top .rt_right :nth-child(2) {
  font-size: 24rpx;
  color: #999999;
  text-decoration: line-through;
}
.my_order .content .co_item .bottom .right .right_top .rt_right :nth-child(3) {
  color: #999999;
}
.my_order .content .co_item .bottom .right .right_bottom {
  margin-top: 40rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
}
.my_order .content .co_item .bottom .right .right_bottom ._span {
  font-size: 24rpx !important;
}
