  -webkit-justify-content: center;
          justify-content: center;
  margin-top: 40rpx;
}
.tutor .inset_box .inset_item6 :nth-child(1) {
  color: #333333;
}
.tutor .inset_box .inset_item6 :nth-child(2) {
