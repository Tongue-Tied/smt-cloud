.notice_bar {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  vertical-align: middle;
  width: 90%;
  margin: 0 auto;
  height: 40rpx;
  -webkit-transform: translate(0, -100rpx);
          transform: translate(0, -100rpx);
  padding-top: 60rpx;
}
